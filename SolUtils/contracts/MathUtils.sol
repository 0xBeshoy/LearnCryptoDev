pragma solidity >=0.8.8;

contract MathUtils {
    function add(uint256 a1, uint256 b1) public pure returns (uint256) {
        return (a1 + b1);
    }

    function sub(uint256 a1, uint256 b1) public pure returns (uint256) {
        return (a1 - b1);
    }

    function mult(uint256 a1, uint256 b1) public pure returns (uint256) {
        return (a1 * b1);
    }

    function div(uint256 a1, uint256 b1) public pure returns (uint256) {
        return (a1 * b1);
    }
}
